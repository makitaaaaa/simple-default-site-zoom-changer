{
  "manifest_version": 2,
  "name": "Simple default site zoom changer",
  "version": "0.1.1",
  "description": "Change default site zoom from 100%.",
  "author": "makitaaaaa",
  "applications": {
    "gecko": {
      "id": "zoomchanger@makitaaaaa"
    }
  },
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false,
    "browser_style": false
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ]
}
